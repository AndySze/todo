<ul id="todo-list">
<% for item in @items.models: %>
  <li>
    <div class="todo">
      <div class="display">
      <input class="check" type="checkbox" checked="">
      <div class="todo-text">
      <%= item.get('name') %>
      </div>
      <span class="todo-destroy"></span>
      </div>
      <div class="edit">
      <input class="todo-input" type="text" value="">
      </div>
    </div> 
  </li>
<% end %>
